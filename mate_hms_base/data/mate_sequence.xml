<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_mate_patient" model="ir.sequence">
            <field name="name">HMS Patient</field>
            <field name="code">mate_hms.patient</field>
            <field name="prefix">HMS</field>
            <field name="padding">3</field>
            <field name="mate_auto_create" eval="True" />
            <field name="company_id" eval="False" />
        </record>

        <record id="seq_mate_physician" model="ir.sequence">
            <field name="name">HMS Physician</field>
            <field name="code">mate_hms.physician</field>
            <field name="prefix">DR</field>
            <field name="padding">3</field>
            <field name="mate_auto_create" eval="True" />
            <field name="company_id" eval="False" />
        </record>

    </data>
</odoo>